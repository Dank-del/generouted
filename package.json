{
  "name": "generouted",
  "version": "1.5.1",
  "description": "Generated client-side file-based routes for React and Vite",
  "author": "Omar Elhawary <oedotme@gmail.com> (https://omarelhawary.me)",
  "license": "MIT",
  "repository": "https://github.com/oedotme/generouted",
  "bugs": "https://github.com/oedotme/generouted/issues",
  "homepage": "https://github.com/oedotme/generouted#readme",
  "keywords": [
    "code-splitting",
    "file-based-routing",
    "generate",
    "nested-layouts",
    "pages",
    "pre-loading ",
    "react",
    "react-location",
    "react-router",
    "react-router-dom",
    "router",
    "routes",
    "vite"
  ],
  "type": "module",
  "exports": {
    "./react-location": {
      "types": "./src/react-location.tsx",
      "import": "./src/react-location.tsx"
    },
    "./react-router": {
      "types": "./src/react-router.tsx",
      "import": "./src/react-router.tsx"
    }
  },
  "typesVersions": {
    "*": {
      "react-location": [
        "./src/react-location.tsx"
      ],
      "react-router": [
        "./src/react-router.tsx"
      ]
    }
  },
  "files": [
    "src"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "format": "prettier --write 'src/**/*.{ts,tsx}'"
  },
  "devDependencies": {
    "@tanstack/react-location": "^3.7.4",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "prettier": "^2.7.1",
    "react-router-dom": "^6.4.1",
    "typescript": "^4.8.3",
    "vite": "^3.1.2"
  },
  "peerDependencies": {
    "vite": ">=3"
  }
}
